{
  "name": "@dvhb/icons",
  "version": "0.1.5",
  "description": "dvhb icons toolset",
  "keywords": [
    "oclif"
  ],
  "homepage": "https://github.com/dvhb/icons",
  "bugs": "https://github.com/dvhb/icons/issues",
  "repository": "dvhb/icons",
  "license": "MIT",
  "author": "Anton Savoskin @sairus2k",
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "bin": {
    "dvhb-icons": "./bin/run"
  },
  "scripts": {
    "lint": "tslint -p tsconfig.json -c tslint.json -t verbose",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "postpack": "rm -f oclif.manifest.json && rm -f tsconfig.tsbuildinfo",
    "semantic-release": "semantic-release",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "posttest": "tslint -p test -t stylish",
    "version": "oclif-dev readme && git add README.md"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.tsx?": [
      "tslint --fix",
      "git add"
    ]
  },
  "prettier": {
    "printWidth": 120,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "dependencies": {
    "@oclif/command": "^1.5.19",
    "@oclif/config": "^1.13.3",
    "@oclif/plugin-help": "^2.2.1",
    "dotenv": "^8.2.0",
    "figma-js": "^1.8.5",
    "svgo": "^1.3.2",
    "tslib": "^1.9.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "@dvhb/tslint-config": "^1.0.9",
    "@oclif/dev-cli": "^1.22.2",
    "@oclif/semantic-release": "^3.1.1",
    "@oclif/test": "^1.2.5",
    "@types/chai": "^4.2.5",
    "@types/dotenv": "^8.2.0",
    "@types/mocha": "^5.2.6",
    "@types/node": "^12.12.14",
    "@types/svgo": "^1.3.0",
    "chai": "^4.2.0",
    "globby": "^10.0.1",
    "husky": "^3.1.0",
    "lint-staged": "^9.4.3",
    "mocha": "^6.2.2",
    "nyc": "^14.1.0",
    "semantic-release": "^15.13.31",
    "ts-node": "^8.5.2",
    "tslint": "^5.20.1",
    "typescript": "^3.7.2"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "oclif": {
    "commands": "./lib/commands",
    "bin": "dvhb-icons",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "release": {
    "extends": "@oclif/semantic-release",
    "plugins": [
      [
        "@semantic-release/git",
        {
          "assets": [
            "CHANGELOG.md",
            "package.json",
            "package-lock.json",
            "README.md"
          ],
          "message": "chore(release): ${nextRelease.version} [skip ci]\\n\\n${nextRelease.notes}"
        }
      ]
    ]
  }
}
