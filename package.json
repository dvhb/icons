{
  "name": "@dvhb/icons",
  "description": "dvhb icons toolset",
  "version": "0.0.0",
  "author": "Anton Savoskin @sairus2k",
  "bin": {
    "dvhb-icons": "./bin/run"
  },
  "bugs": "https://github.com/dvhb/icons/issues",
  "dependencies": {
    "@oclif/command": "^1.5.13",
    "@oclif/config": "^1.12.12",
    "@oclif/plugin-help": "^2.1.6",
    "tslib": "^1.9.3"
  },
  "devDependencies": {
    "@dvhb/tslint-config": "^1.0.6",
    "@oclif/dev-cli": "^1.22.0",
    "@oclif/test": "^1.2.4",
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.6",
    "@types/node": "^10.14.5",
    "chai": "^4.2.0",
    "globby": "^8.0.2",
    "husky": "^2.2.0",
    "lint-staged": "^8.1.6",
    "mocha": "^5.2.0",
    "nyc": "^14.1.0",
    "ts-node": "^8.1.0",
    "tslint": "^5.16.0",
    "typescript": "^3.4.5"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/dvhb/icons",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "dvhb-icons",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "dvhb/icons",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "tslint -p test -t stylish",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts",
  "prettier": {
    "printWidth": 120,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.tsx?": [
      "tslint --fix",
      "git add"
    ]
  }
}
